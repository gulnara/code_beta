<p id="notice"><%= notice %></p>

<p>
  <strong>Solution for Problem:</strong>
<%= @problem.title %>
<%= @problem.id %>
</p>

<p>
  <strong>Answer:</strong>
  <%= @solution.answer %>
  <%= @solution.id %>
</p>

<% if @solution.user == current_user %>
	<%= link_to 'Edit', edit_problem_solution_path(@problem, @solution) %> 
<% end %>
<%= link_to 'Back', problem_path(@problem) %>
